{
  "name": "food-node-api",
  "version": "0.0.1",
  "engines": {
    "node": ">=10.0"
  },
  "description": "Food node api",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon ./src/bin/www -L",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "db:reset": "npm run migrate:undo && npm run migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "db:build": "npm run db:reset && npm run db:seed",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.24.0",
    "nodemon": "^1.18.4",
    "password-hash": "^1.2.2",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^21.17.0",
    "faker": "^4.1.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
